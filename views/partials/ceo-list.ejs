<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Apple CEOs</title>
  </head>
  <body>
    <ul>
      <% db.forEach(element => { %>
      <li><a href="/ceos/<%=`${element.slug}` %>"><%= element.name %> </a></li>
      <% }) %>
    </ul>
  </body>
</html>
